<div class ="text-center"> 
    <h1>Login</h1>
</div>

<%= form_for(:session ,url: login_path) do |f| %>
  
  <div>
     <%= f.label :email, "Email"%>
     <%= f.email_field :email %>
  </div>
  <div>
     <%= f.label :password, "Password"%>
     <%= f.password_field :password %>
  </div>
  <%= f.submit "login" %>
<% end %>